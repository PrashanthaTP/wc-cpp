{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/include",
            "${workspaceFolder}/src",
            "${workspaceFolder}/build/_deps/googletest-src/googletest/include",
            "${workspaceFolder}/build/_deps/googletest-src/googlemock/include"
        ]
    },
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${myIncludePath}"
            ],
            "cppStandard": "gnu++17",
            "browse": {
                "path": [
                    "${myIncludePath}",
                    "${workspaceFolder}"
                ]
            }
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}
